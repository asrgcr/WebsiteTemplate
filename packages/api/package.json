{
  "name": "@website/api",
  "version": "1.0.0",
  "description": "API Template",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "types": "src/external-types.ts",
  "scripts": {
    "build": "tsc --build tsconfig.lib.json",
    "typecheck": "tsc",
    "start": "tsx watch scripts/run-dev-server.ts"
  },
  "nx": {
    "targets": {
      "build": {
        "outputs": [
          "{workspaceRoot}/build/api-lambda/index.mjs"
        ]
      },
      "start": {
        "dependsOn": []
      }
    }
  },
  "dependencies": {
    "@trpc/server": "^11.1.2",
    "aws-lambda": "^1.0.7",
    "@types/aws-lambda": "^8.10.150",
    "express": "^5.1.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/cors": "^2.8.17",
    "cors": "^2.8.5"
  }
}
