{
  "name": "@website/cdk",
  "version": "1.0.0",
  "description": "The CDK managed infrastructure for the template website",
  "license": "UNLICENSED",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "amzn-cdk": "cd ../.. && amzn-cdk",
    "build": "cd ../.. && cdk synth",
    "cdk": "cd ../.. && cdk",
    "check-format": "prettier . --check",
    "clean": "rm -rf build coverage node_modules"
  },
  "nx": {
    "targets": {
      "amzn-cdk": {
        "dependsOn": [
          "build"
        ]
      },
      "build": {
        "dependsOn": [
          "^build"
        ],
        "outputs": [
          "{workspaceRoot}/build/cdk.out"
        ]
      },
      "cdk": {
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "dependencies": {
    "aws-cdk-lib": "^2.205.0",
    "constructs": "^10.4.2"
  },
  "devDependencies": {
    "aws-cdk": "^2.1001.0",
    "compressjs": "^1.0.3"
  }
}
